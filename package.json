{
  "name": "my-capri-react-site",
  "private": true,
  "description": "Capri with TanStack Router",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "npm run build && npm run preview",
    "dev": "concurrently 'npm:dev:*'",
    "dev:build": "vite dev",
    "dev:generate": "tsr watch",
    "generate": "tsr generate",
    "build": "npm run generate && vite build && vite build --ssr",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-router": "^0.0.1-beta.38",
    "@tanstack/router-cli": "^0.0.1-beta.36",
    "@vitejs/plugin-react": "^1.3.2",
    "classname-variants": "^1.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-render-to-string": "*"
  },
  "devDependencies": {
    "@capri-js/react": "*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "vite": "^3.0.0"
  }
}
